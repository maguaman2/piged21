<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.empresa.PrimaryController">
   <children>
      <VBox alignment="CENTER" prefHeight="314.0" prefWidth="412.0" spacing="20.0">
         <children>
            <Label text="Ingrese su codigdo" />
            <TextField fx:id="txtUserName" maxWidth="1.7976931348623157E308" prefHeight="25.0" prefWidth="296.0" />
            <TextField fx:id="txtPassword" />
            <Label fx:id="lbMensaje" prefHeight="17.0" prefWidth="191.0" VBox.vgrow="ALWAYS" />
            <Button fx:id="primaryButton" onAction="#switchToSecondary" text="Ingresar" />
            <Button fx:id="btnUno" mnemonicParsing="false" onAction="#switchToCreditos" text="Creditos" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </VBox>
   </children>
</HBox>
