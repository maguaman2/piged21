<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="314.0" prefWidth="412.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.empresa.PrimaryController">
   <children>
      <Label text="Ingrese su codigdo" />
      <TextField fx:id="txtCodigo" maxWidth="1.7976931348623157E308" />
      <TextField fx:id="txtCodigoDos" />
      <Label fx:id="lbMensaje" prefHeight="17.0" prefWidth="191.0" VBox.vgrow="ALWAYS" />
      <Button fx:id="primaryButton" onAction="#switchToSecondary" text="Ingresar" />
      <Button fx:id="btnUno" mnemonicParsing="false" onAction="#switchToCreditos" text="Creditos" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
